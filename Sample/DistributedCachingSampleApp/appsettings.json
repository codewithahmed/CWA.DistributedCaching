{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",

  "CacheSettings": {
    "PreferRedis": true,
    "RedisHost": "localhost:6379"
  },
  "CacheEntryOptions": {
    "AbsoluteExpirationInHours": 8,
    "SlidingExpirationInMinutes": 60
  }
}